@article{IET,
	author = {Modesto, Rodrigo Augusto and Oliveira da Silva, Sérgio Augusto and {de Oliveira Junior}, Azauri Albano},
	journal = {IET Power Electronics},
	title = {Power quality improvement using a dual unified power quality conditioner/uninterruptible power supply in three-phase four-wire systems},
	year={2015},
	volume = {8},
	number = {9},
	pages = {1595-1605},
	keywords = {power supply quality, uninterruptible power supplies, power system control, power conversion harmonics, compensation, PWM power convertors, power grids, voltage control, electric current control, harmonics suppression, power quality improvement, three-phase four-wire systems, 3P4W systems, dual compensation strategy, power quality conditioners, unified PQC, UPQC, control strategy, uninterruptible power supply system, UPS system, load harmonic currents, load reactive power, load unbalance compensation, utility voltage unbalance compensation, utility voltage harmonic suppression, load voltages, pulse width modulation, PWM converters, sinusoidal voltage source, utility grid, sinusoidal current source, voltage controllers, current controllers, synchronous rotating reference frame, 3D space vector modulation technique, digital signal processor},
	%doi = {https://doi.org/10.1049/iet-pel.2014.0734},
	%url = {https://ietresearch.onlinelibrary.wiley.com/doi/abs/10.1049/iet-pel.2014.0734},
	eprint = {https://ietresearch.onlinelibrary.wiley.com/doi/pdf/10.1049/iet-pel.2014.0734},
	abstract = {Using a dual compensation strategy, this study proposes the analysis and implementation of active power quality conditioners (PQCs), which are applied for power quality improvement in three-phase four-wire (3P4W) systems. By means of similar control strategy use, the topology employed to implement the PQCs can operate as unified PQC (UPQC), or, through some circuit modifications, as uninterruptible power supply (UPS) system. Considering 3P4W systems, the use of the dual compensation strategy into the PQCs comprised of two four-leg (4-Leg) converters is the main contribution of this study. Different from the conventional strategies, in the dual compensation strategy, the 4-Leg converter connected in parallel with the load is controlled to operate as a sinusoidal voltage source, whereas the 4-Leg converter connected in series between the grid and the load is controlled to operate as a sinusoidal current source. Both the voltage and current controllers are implemented into the synchronous rotating reference frame, and the converters use the three-dimensional space vector modulation technique. Experimental tests, based on a digital signal processor, are performed in order to validate the theoretical development and to verify both static and dynamic performance of the PQC operating as UPQC, as well as UPS system.},
	year = {2015}
}


